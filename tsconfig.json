{
	"compilerOptions": {
		"target": "es5",
		"lib": ["es5", "es6", "dom"],
		"sourceMap": false,
		"declaration": false,
		"module": "commonjs",
		"outDir": "./dist",
		"removeComments": true,
		"typeRoots": [
			"node_modules/@types"
		],
		"types": [
			"node"
		],
		"noStrictGenericChecks": true
	},
	"include": [
		"src/*.ts"
	],
	"exclude": [
		"node_modules"
	]
}
